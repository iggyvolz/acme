{
    "name": "kelunik/acme",
    "description": "ACME library written in PHP.",
    "license": "MIT",
    "authors": [
        {
            "name": "Niklas Keller",
            "email": "me@kelunik.com"
        }
    ],
    "keywords": [
        "ACME",
        "letsencrypt",
        "certificate",
        "https",
        "encryption",
        "ssl",
        "tls"
    ],
    "autoload": {
        "psr-4": {
            "Kelunik\\Acme\\": "lib"
        },
        "files": [
            "lib/functions.php"
        ]
    },
    "require": {
        "php": ">=7.2",
        "ext-json": "*",
        "ext-openssl": "*",
        "amphp/amp": "^2",
        "amphp/http-client": "^4",
        "sabre/uri": "^1",
        "psr/log": "^1",
        "kelunik/certificate": "^1.1",
        "amphp/file": "^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8 || ^9",
        "amphp/phpunit-util": "^1.0",
        "amphp/php-cs-fixer-config": "dev-master"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1-dev"
        }
    }
}
